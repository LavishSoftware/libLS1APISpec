<Window x:Class="LS1ReferenceExporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LS1ReferenceExporter"
        mc:Ignorable="d"
        Title="LS1 Reference Exporter" Width="300" Height="200">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10,5">
            <Button Content="JMB-Session" Name ="buttonJMBSession" Click="buttonJMBSession_Click" Margin="0,5,0,5" Padding="5,2" />
            <Button Content="JMB-Uplink" Name="buttonJMBUplink" Click="buttonJMBUplink_Click" Margin="0,5,0,5" Padding="5,2" />
            <Button Content="IS-Session" Name="buttonISSession" Click="buttonISSession_Click" Margin="0,5,0,5" Padding="5,2"/>
            <Button Content="IS-Uplink" Name="buttonISUplink" Click="buttonISUplink_Click" Margin="0,5,0,5" Padding="5,2"/>
        </StackPanel>
    </Grid>
</Window>
